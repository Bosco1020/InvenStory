{
  "name": "invenstory-back-end",
  "version": "1.0.0",
  "description": "InvenStory-Back-End",
  "main": "src/index.js",
  "type": "module",
  "directories": {
    "test": "test",
    "start": "nodemon src/index,js"
  },
  "scripts": {
    "prod": "NODE_ENV=prod nodemon src/index.js",
    "start": "NODE_ENV=dev nodemon src/index.js",
    "start-win": "SET NODE_ENV=dev&&nodemon&&src/index.js",
    "test": "NODE_ENV=test mocha --timeout 10000 --exit --recursive",
    "test-win": "SET NODE_ENV=test&&mocha --timeout 10000 --exit --recursive",
    "coverage": "NODE_ENV=test c8 -r html npm test",
    "coverage-win": "SET NODE_ENV=test&&c8 -r html npm run test-win"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.3"
  },
  "devDependencies": {
    "c8": "^10.1.2",
    "chai": "^5.1.1",
    "mocha": "^10.4.0",
    "sinon": "^18.0.0",
    "supertest": "^7.0.0"
  }
}
